                                    <textarea id="equipmentComments3" asp-for="@Model.Equipment[i].Comments" rows="3" placeholder="" type="text" maxlength="150">@Model.Equipment[i].Comments</textarea>
                                </td>
                            </tr>
                                itemNum++;
                                i++;
                                Model.Equipment.Add(new LearjetProofEquipment());
                            <tr>
                                <td class="rowName">
                                        @itemNum . @Equipment.Fields[i].Descr
                                </td>
                                <input asp-for="@Model.Equipment[i].DescriptionId" value="@Equipment.Fields[i].Id" type="hidden" />
                                <td class="emptyBlock">
                                </td>
                                <td>
                                    <select id="equipmentRevStatusQty4" asp-for="@Model.Equipment[i].RevStatusQTY" value="@Model.Equipment[i].RevStatusQTY" required onchange="RequireIfSelected('equipmentRevStatusQty4','No','equipmentComments4')">
                                        <option disabled selected>-Select-</option>
                                        <option value="Not Onboard">Not Onboard</option>
                                        <option value="Onboard">Onboard</option>
                                    </select>
                                </td>
                                <td>
                                    <textarea id="equipmentComments4" asp-for="@Model.Equipment[i].Comments" rows="3" placeholder="" type="text" maxlength="150">@Model.Equipment[i].Comments</textarea>
                                </td>
                            </tr>
                            }
                        }
                </tbody>

                }

        </table>
        <!--Additional Items-->
        <table class="table" slot="table">
                @{

                }
            <thead>
                <tr>
                    <th class="h3" colspan="7">Additional Items</th>
                </tr>
                <tr>
                    <th class="h4">Item</th>
                    <th class="h4" rowspan="1">Date (if appl.)</th>
                    <th class="h4" rowspan="1"> Rev Status / QTY</th>
                    <th class="h4" rowspan="1">Comments</th>
                </tr>
            </thead>
            <tbody>
                    @for (int i = 1; i <= 5; i++)
                    {
                    <tr>
                        <td>
                            <input placeholder="Item Name" name="addItemName" type="text" maxlength="150" />
                        </td>
                        <td>
                            <input placeholder="" name="date" value="@DateTime.Today.ToString("yyyy-MM-dd")" type="date" maxlength="150" />
                        </td>
                        <td>
                            <textarea rows="3" placeholder="" name="Qty" type="text" maxlength="150"></textarea>
                        </td>
                        <td>
                            <textarea rows="3" placeholder="" name="comments" type="text" maxlength="150"></textarea>
                        </td>
                    </tr>
                    }
            </tbody>
        </table>

        <div style="padding:8px; display:flex; flex-direction:row; border:1px solid black;">
            <div style="max-width:75%;display:flex; border-right:1px solid black; ">
                I hereby certify that I have checked the aircraft and declare this document a true reflection of the status of the aircraft equipment and documentation.
                I also acknowledge that the crew has read all documents on ASQS under the document Module
            </div>
            <div>
                Signed By:  @Model.CompletedBy
            </div>
        </div>

    </nac-table-pager>
    <input class="btn btn-primary" type="submit" value="Submit" style="position:initial;" />

</form>
