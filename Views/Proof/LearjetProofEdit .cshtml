<script>
    function CalculateOffsetDate(src, target, targetInput, offset) {
        //Add years
        var srcElement = document.getElementById(src);
        var targetElement = document.getElementById(target);
        var targetInputElement = document.getElementById(targetInput);
        var offsetYears = parseInt(offset);
        if (srcElement) {
            var date = new Date(srcElement.value);
            date.setFullYear(date.getFullYear() + offsetYears);
            const year = date.getFullYear();
            const month = ('0' + (date.getMonth() + 1)).slice(-2);
            const day = ('0' + date.getDate()).slice(-2);
            const formattedDate = `${year}-${month}-${day}`;

            targetElement.value = formattedDate;
            targetInputElement.value = formattedDate;
        }
    }
    function RequireIfSelected(src, value, target) {
        var srcElement = document.getElementById(src);
        var targetElement = document.getElementById(target);
        if (srcElement) {
            var srcValue = srcElement.value;
            var isArray = Array.isArray(value);
            if (isArray) {
                for (var index = 0; index < value.length; index++) {
                    var flag = compareValues(srcValue, value[index]);
                    flagElement(targetElement, flag);
                    if (flag)
                        return;
                }
            } else {
                var flag = compareValues(srcValue, value);
                flagElement(targetElement, flag);
            }
        }
    }
    function compareValues(value0, value1) {
        return (value0 == value1);
    }
    function flagElement(target, flag) {
        if (flag) {
            target.setAttribute('required', true);
            //Focus on the element
            target.classList.add('alertFocus');
            target.focus();
        } else {
            target.classList.remove('alertFocus');
            target.removeAttribute('required');
        }
    }


    function onMultipleSelect(src, target) {
        var select = document.getElementById(src);
        var t = document.getElementById(target);
        var values = "";
        select.options[0].selected = false;
        for (var i = 1; i < select.options.length - 1; i++) {
            if (select.options[i].selected) {
                values = values + select.options[i].value + ", ";
            }
        }
        t.value = values;
        console.log(t.value)
    }
    function aircraftSelect(src) {
        var target = document.querySelector('nac-table-pager');
        var selection = document.getElementById(src);
        var aircraft = selection.selectedOptions[0].innerHTML;
        console.log(aircraft)

        target.setAttribute('aircraft', aircraft);
    }
    function showToolTip(msg, target) {
        targetElement.setAttribute('title', 'Please Provide A Comment');
        //
        var x = targetElement.getBoundingClientRect().left
        var x = targetElement.getBoundingClientRect().left
    }
    function onAircraftSelect(src, target) {
        var srcElement = document.getElementById(src);
        var targetElement = document.getElementById(target);
        targetElement.innerText = srcElement.options[srcElement.selectedIndex].text;
    }

    function toggleDate(checked, dateId, hiddenId, attr) {
        let myDate = document.getElementById(dateId);
        let myHidden = document.getElementById(hiddenId);
        var a = `@@${attr}`
        if (checked) {
            myDate.disabled = true;
            myHidden.setAttribute('asp-for', a);
            myDate.removeAttribute('asp-for');
            myHidden.value = "NULL";

        } else {
            myDate.disabled = false;
            myHidden.value = myDate.value;
            myDate.setAttribute('asp-for', a);
            myHidden.removeAttribute('asp-for');
        }
    }

</script>
