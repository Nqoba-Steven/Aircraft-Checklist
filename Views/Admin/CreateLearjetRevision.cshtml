@using NAC_Aircraft_Checklist.Models.Entry.Revisions;
@model LearjetRevision;
@{
    
}

<script>
    function openPage(target) {
        window.location = target;
    }
</script>
<h1>Create Learjet Revision</h1>
<form method="Post" action="/Admin/BuildLearjetTableMaster">
    <table>
        <tr>
            <td>Revision Number</td>
            <td>
                <input asp-for="@Model.RevisionNumber" type="number" />
            </td>
            <td>Revision Date</td>
            <td>
                <input asp-for="@Model.Date" type="date" />
            </td>
        </tr>
    </table>
    <button type="submit">Create</button>
</form>
<button onclick="openPage('/Admin/')">Cancel</button>
