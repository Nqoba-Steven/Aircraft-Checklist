                <th class="h4">Comments</th>
            </tr>
        </thead>

        <tbody>
            @functions {
                public string getColor(string status)
                {

                    switch (status.ToLower())
                    {
                        case "unserviceable":
                        case "onboard unserviceable":
                        case "yes unserviceable":
                            return "color:fff;background-color:#f44336;";

                        case "expired":
                            return "color:fff;background-color:#f44336;";
                        case "not onboard":
                            return "color:fff;background-color:#f44336;";
                        case "bad condition":
                            return "color:fff;background-color:#f44336;";
                        case "onboard":
                        case "acceptable":
                        case "2x Servicable":
                        case "1x Servicable":
                        case "1x Orange Servicable":
                        case "current":
                        case "onboard serviceable":
                        case "serviceable":
                        case "yes serviceable":
                        case "yes good condition":
                        case "good condition":
                            return "background-color:#89e36a;";
                        default:
                            return "";
                    }

                }
            }
            @{
                var itemNum = 1;
                var i = 0;
                if (Exterior != null)
                {
                    foreach (var item in Model.Exteriors)
                    {
                        <tr>
                            <td class="rowName">
                                @itemNum . @Exterior.Fields[i].Descr
                            </td>
                            @if (@item.Status == null)
                            {
                                <td class="emptyBlock">
                                </td>
                            }
                            else
                            {
                                <td style="@getColor(@item.Status)">
                                    @item.Status
                                </td>
                            }
                            @if (@item.Comments == null)
                            {
                                <td class="emptyBlock">
                                </td>
                            }
                            else
                            {
                                <td>
                                    @item.Comments
                                </td>
                            }
                        </tr>
                        itemNum++;
                        i++;

                    }
                }
            }

        </tbody>

