                            @if (@item.DateIfApplicable == null)
                            {
                                <td class="emptyBlock">
                                </td>
                            }
                            else
                            {
                                <td>
                                    @item.DateIfApplicable.Value.ToString("yyyy-MM-dd")
                                </td>
                            }
                            @if (@item.RevisionText == null)
                            {
                                <td class="emptyBlock">
                                </td>
                            }
                            else
                            {
                                <td>
                                    @item.RevisionText
                                </td>
                            }
                            @if (@item.RevStatusQTY == null)
                            {
                                <td class="emptyBlock">
                                </td>
                            }
                            else
                            {
                                <td style="@getColor(@item.RevStatusQTY)">
                                    @item.RevStatusQTY
                                </td>
                            }
                            @if (@item.Comments == null)
                            {
                                <td class="emptyBlock">
                                </td>
                            }
                            else
                            {
                                <td>
                                    @item.Comments
                                </td>
                            }
                        </tr>
                        itemNum++;
                        i++;
                    }

                }
            }
        </tbody>
    </table>

    <!--Equipment-->
    <table slot="table" class="table table-striped">
        @{
            var Equipment = ViewData["Equipment"] as Table<LearjetEquipmentMaster>;
        }
        <thead clas="thead-dark">
            <tr>
                <th class="h4" colspan="7">@Equipment.Header</th>
            </tr>
            <tr>
                <th></th>
                <th class="h5" rowspan="1">Date (if appl.)</th>
                <th class="h5" rowspan="1">QTY</th>
                <th class="h5" rowspan="1">Comments</th>
            </tr>
        </thead>
        <tbody>


            @{
                if (Equipment != null)
                {

                    itemNum = 1;
                    i = 0;
                    foreach (var item in Model.Equipment)
                    {

                        <tr>
                            <td class="rowName">
                                @itemNum . @Equipment.Fields[i].Descr
                            </td>
                            @if (@item.DateIfApplicable == null)
                            {
                                <td class="emptyBlock">
                                </td>
                            }
                            else
                            {
                                <td>
                                    @item.DateIfApplicable.Value.ToString("yyyy-MM-dd")
                                </td>
                            }

                            @if (@item.RevStatusQTY == null)
                            {
                                <td class="emptyBlock">
                                </td>
                            }
                            else
                            {
                                <td style="@getColor(@item.RevStatusQTY)">
                                    @item.RevStatusQTY
                                </td>
                            }
                            @if (@item.Comments == null)
                            {
                                <td class="emptyBlock">
                                </td>
                            }
                            else
                            {
                                <td>
                                    @item.Comments
                                </td>
                            }
                        </tr>
                        itemNum++;
                        i++;
                    }

                }
            }
        </tbody>
    </table>

    <!--Additional Items-->
    @{
        if (Model.AdditionalItems != null && Model.AdditionalItems.Count > 0)
        {
            <table slot="table" class="table table-striped">
                <thead class="thead-dark">
                    <tr>
                        <th class="h4" colspan="7">Additional Items</th>
                    </tr>
                    <tr>
                        <th class="h5">Item</th>
                        <th class="h5" rowspan="1">Date (if appl.)</th>
                        <th class="h5" rowspan="1"> Rev Status / QTY</th>
                        <th class="h5" rowspan="1">Comments</th>
                    </tr>
                </thead>

                <tbody>

                    @foreach (var item in Model.AdditionalItems)
                    {
                        <tr>
                            @if (@item.ItemName == null)
                            {
                                <td class="emptyBlock">
                                </td>
                            }
                            else
                            {
                                <td>
                                    @item.ItemName
                                </td>
                            }
                            @if (@item.DateIfApplicable == null)
                            {
                                <td class="emptyBlock">
                                </td>
                            }
                            else
                            {
                                <td>
                                    @item.DateIfApplicable.Value.ToString("yyyy-MM-dd")
                                </td>
                            }
                            @if (@item.RevStatusQTY == null)
                            {
                                <td class="emptyBlock">
                                </td>
                            }
                            else
                            {
                                <td style="@getColor(@item.RevStatusQTY)">
                                    @item.RevStatusQTY
                                </td>
                            }
                            @if (@item.Comments == null)
                            {
                                <td class="emptyBlock">
                                </td>
                            }
                            else
                            {
                                <td>
                                    @item.Comments
                                </td>
                            }
                        </tr>

                    }

                </tbody>
            </table>
        }

    }

    <!--Disclaimer Table-->
    <!--Disclaimer Table-->
